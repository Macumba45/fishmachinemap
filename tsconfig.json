{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        "src/app/api/signup.ts",
        "src/app/api/auth/NextAuth.js",
        "pages/api/markers/marker.ts",
        "src/app/utils/cloudinary.ts",
        "src/app/utils/formidable.js",
        "pages/api/upload.js",
        "webpack.config.js"
, "pages/api/markers/autoDeleteMarkers.ts"    ],
    "exclude": ["node_modules"]
}
